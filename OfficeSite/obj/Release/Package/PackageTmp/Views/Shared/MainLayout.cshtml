                                                <li class="menu-item"><a href="@Url.Action("PricingPlan","Admin")">Pricing Plan</a></li>
                                                <li class="menu-item"><a href="@Url.Action("Services","Admin")">Our Services</a></li>
                                                <li class="menu-item"><a href="@Url.Action("Collaboration","Admin")">Collabration</a></li>
                                                <li class="menu-item"><a href="@Url.Action("Career","Admin")">Career</a></li>
                                                <li class="menu-item"><a href="@Url.Action("Legal","Admin")">Legal</a></li>
                                                <li class="menu-item"><a href="@Url.Action("FairUse","Admin")">Fair Use</a></li>
                                            </ul>
                                        </li>
                                        <li class="menu-item">
                                            <a href="@Url.Action("Contact","Admin")">Contact</a>
                                            <ul class="sub-menu">
                                                <li class="menu-item"><a href="@Url.Action("GuestPostingLanding","Admin")">Community Blog</a></li>
                                                <li class="menu-item"><a href="@Url.Action("GuestPosting","Admin")">Guest Posting</a></li>
                                                <li class="menu-item"><a href="@Url.Action("FreatureArticle","Admin")">Feature Your Article</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="navbar-right menu-right">
                                <ul class="d-flex align-items-center list-inline m-0">
                                    <li class="nav-item nav-icon">
                                        <input type="text" id="searchBox" placeholder="search what you want...." />
                                        <div id="imageContainer"></div>
                                    </li>
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
