    <!-- Header End -->
    <!-- Slider Start -->
    @RenderBody()
    <!-- Slider End -->
    <!-- MainContent End-->
    <!-- back-to-top -->
    <div id="back-to-top">
        <a class="top" href="#top" id="top"> <i class="fa fa-angle-up"></i> </a>
    </div>
    <!-- back-to-top End -->
    <footer id="contact" class="footer-one iq-bg-dark">
        <!-- Address -->
        <div class="footer-top">
            <div class="container-fluid">
                <div class="row footer-standard">
                    <div class="col-lg-12 col-md-12 mt-4 mt-lg-0 text-center">
                        <h6 class="footer-link-title">
                            Follow Us :
                        </h6>
                        <ul class="info-share">
                            <li><a target="_blank" href="https://www.facebook.com/EInfoMovies"><i class="fa fa-facebook"></i></a></li>
                            <li><a target="_blank" href="https://twitter.com/EinfoGloba41811"><i class="fa fa-twitter"></i></a></li>
                            <li><a target="_blank" href="https://www.linkedin.com/company/e-info-movies/posts/?feedView=all&viewAsMember=true"><i class="fa fa-linkedin"></i></a></li>
                            <li><a target="_blank" href="https://www.pinterest.com/einfoglobal/"><i class="fa fa-pinterest"></i></a></li>
                            <li><a target="_blank" href="https://www.quora.com/profile/E-Info-Global"><i class="fa fa-quora"></i></a></li>
                        </ul>
                        <div class="textwidget">
                            <a href="https://maksoninnovations.com/" target="_blank">
                                <p>
                                    <small>
                                        Powered by Makson Innovations 2022 - @DateTime.Now.Year
                                    </small>
                                </p>
                            </a>
                            <p>
                                <small>
                                    This site is affiliated with Amazon Associates and Apple Tv +
                                </small>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Address END -->
    </footer>
    <!-- jQuery, Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/corejs-typeahead/0.11.1/typeahead.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/typeahead.js/0.11.1/typeahead.jquery.min.js"></script>
