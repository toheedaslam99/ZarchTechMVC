
@{
    ViewBag.Title = "Collaboration";
    Layout = "~/Views/Shared/MainLayout.cshtml";
}
<div class="iq-breadcrumb-one  iq-bg-over " style="background-image: url(../../NewTheme/images/about-us/01.jpg);">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-sm-12">
                <nav aria-label="breadcrumb" class="text-center iq-breadcrumb-two">
                    <h2 class="title">Collaboration</h2>
                    <ol class="breadcrumb main-bg">
                        <li class="breadcrumb-item"><a href="@Url.Action("LandingPage","Design")">Home</a></li>
                        <li class="breadcrumb-item active">Collaboration</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<!-- mastermind start -->
<main id="main" class="site-main">
    <div class="container-fluid p-0">
        <div class="map-container" style=" left: 0px;">
            <div class="container">
                <div class="row about-us-row text-center align-items-center">
                    <div class="col-md-6">
                        <div class=" text-left iq-title-box iq-title-default">
                            <h3 class="title">Your Gateway To Taking Your Production House And Brand To New Heights</h3>
                            <div class="blog-button">
                                <a class="button-link" href="@Url.Action("Contact","Admin")">Contact Us<i class="fa fa-angle-right" aria-hidden="true"></i></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="text-center">
                            <img width="497" height="477" src="~/NewTheme/images/about-us/16.jpg" class=" img-fluid attachment-large size-large" alt="" loading="lazy">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-sm-12">
                <h5 class="title text-center">your hard work at international film festivals with our stream tuner products.</h5>
                <br />
                <div class="row contact-detail text-center">
                    <div class="col-md-4">
                        <div class="icon-box-content">
                            <h4 class="icon-box-title">
                                <span>Increased exposure and visibility for your series on your global scale</span>
                            </h4>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="icon-box-content">
                            <h4 class="icon-box-title">
                                <span>Expand your audience to new markets and demographics</span>
                            </h4>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="icon-box-content">
                            <h4 class="icon-box-title">
                                <span>Increase recognition and prestige through international file festivals submission</span>
                            </h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid p-0">
        <div class="map-container" style=" left: 0px;">
            <div class="container">
                <div class="row about-us-row text-center align-items-center">
                    <div class="col-md-6">
                        <div class=" text-left iq-title-box iq-title-default">
                            <p class="iq-title-desc">When your collaborate with einfomovies.com you're not just signing up for a platform: you're signing up for an experience.</p>
                            <p class="iq-title-desc">Our team of experts is dedicated to helping your acheive your goals and take your productions to the next level. We're passionate about providing you with the best possible service and ensuring your success in the international market.</p>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="text-center">
                            <img width="497" height="477" src="~/NewTheme/images/about-us/17.jpg" class=" img-fluid attachment-large size-large" alt="" loading="lazy">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
