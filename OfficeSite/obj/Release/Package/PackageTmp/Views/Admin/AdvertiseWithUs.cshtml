                                <a class="button-link" href="@Url.Action("Contact","Admin")">Contact Us<i class="fa fa-angle-right" aria-hidden="true"></i></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="text-center">
                            <img width="497" height="477" src="~/NewTheme/images/about-us/10.jpg" class=" img-fluid attachment-large size-large" alt="" loading="lazy">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
