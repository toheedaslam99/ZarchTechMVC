                                            <div class="img-box">
                                                <img src="~/image/@item.Movie1.MovieImage" class="img-fluid" alt="" loading="lazy">
                                            </div>
                                            <div class="block-description">
                                                <div class="hover-buttons">
                                                    <a href="@Url.Action("movie","design",new {specificmovies = item.Movie1.Title, find = item.CategoryID })" role="button" class="btn btn-hover">
                                                        <i class="fa fa-play mr-1" aria-hidden="true"></i>
                                                        play now
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                }
                                @foreach (var Landing in ViewBag.SideBar)
                                {
                                    if (Landing.SeqOrder == "4")
                                    {
                                        if (Landing.AdsType != "Manually")
                                        {
                                            <li class="rc-post-item">
                                                <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8209193384023908"
                                                        crossorigin="anonymous"></script>
                                                <!-- responsive square -->
                                                <ins class="adsbygoogle"
                                                     style="display:block"
                                                     data-ad-client="ca-pub-8209193384023908"
                                                     data-ad-slot="2285067466"
                                                     data-ad-format="auto"
                                                     data-full-width-responsive="true"></ins>
                                                <script>
                                                    (adsbygoogle = window.adsbygoogle || []).push({});
                                                </script>
                                            </li>
                                        }
                                        else
                                        {
                                            
                                                <li class="rc-post-item">
                                                    <div class="thumb">
                                                        <a href="~/design/logs?ID=@Landing.ID&CID=@Landing.CustomerID">
                                                            <img src="~/AdvertiseImage/@Landing.Image" alt="">
                                                        </a>
                                                    </div>
                                                </li>
                                        }
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
