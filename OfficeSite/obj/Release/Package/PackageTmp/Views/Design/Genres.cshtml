<main id="main" class="genres-main">
    <section class="iq-genres-section">
        <div class="container-fluid">
            <div class="iq-main-header d-flex align-items-center justify-content-between mt-5 mt-lg-0">
                <h4 class="main-title">Movie Categories</h4>
            </div>
            <ul id="ListData" class=" row mb-0 list-inline">
                @foreach (var item in Model)
                {
                    <li class="col-xl-3 col-lg-3 col-md-3 col-sm-6 text-center">
                        <a href="@Url.Action("CategoryMovies","Design",new {CateId = item.CategoryName, find = item.CategoryID })">
                            <div class="iq-tag-box watchlist-first">
                                <span class="iq-tag ">@item.CategoryName</span>
                            </div>
                        </a>
                    </li>
                }
            </ul>
        </div>
    </section>
</main>


